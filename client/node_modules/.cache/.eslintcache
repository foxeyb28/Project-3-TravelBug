ow.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n",{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":3,"column":8,"nodeType":"113","messageId":"114","endLine":3,"endColumn":21},{"ruleId":"111","severity":1,"message":"115","line":4,"column":8,"nodeType":"113","messageId":"114","endLine":4,"endColumn":22},{"ruleId":"111","severity":1,"message":"116","line":5,"column":8,"nodeType":"113","messageId":"114","endLine":5,"endColumn":22},{"ruleId":"111","severity":1,"message":"117","line":1,"column":27,"nodeType":"113","messageId":"114","endLine":1,"endColumn":36},{"ruleId":"111","severity":1,"message":"118","line":4,"column":8,"nodeType":"113","messageId":"114","endLine":4,"endColumn":18},{"ruleId":"111","severity":1,"message":"119","line":10,"column":8,"nodeType":"113","messageId":"114","endLine":10,"endColumn":21},{"ruleId":"111","severity":1,"message":"120","line":16,"column":10,"nodeType":"113","messageId":"114","endLine":16,"endColumn":20},{"ruleId":"111","severity":1,"message":"121","line":35,"column":12,"nodeType":"113","messageId":"114","endLine":35,"endColumn":16},{"ruleId":"111","severity":1,"message":"122","line":1,"column":7,"nodeType":"113","messageId":"114","endLine":1,"endColumn":21},{"ruleId":"111","severity":1,"message":"123","line":3,"column":8,"nodeType":"113","messageId":"114","endLine":3,"endColumn":17},{"ruleId":"111","severity":1,"message":"124","line":2,"column":8,"nodeType":"113","messageId":"114","endLine":2,"endColumn":9},{"ruleId":"111","severity":1,"message":"125","line":17,"column":9,"nodeType":"113","messageId":"114","endLine":17,"endColumn":25},{"ruleId":"111","severity":1,"message":"126","line":27,"column":22,"nodeType":"113","messageId":"114","endLine":27,